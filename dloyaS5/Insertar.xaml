<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dloyaS5.Insertar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="VENTANA INSERTAR DATOS!" HorizontalTextAlignment="Center"  FontSize="20"/>

            <Entry x:Name="txtCodigo" Placeholder="Codigo"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre"/>
            <Entry x:Name="txtApellido" Placeholder="Apellido"/>
            <Entry x:Name="txtEdad" Placeholder="Edad"/>

            <Button Text="REGISTRAR" Clicked="Button_Clicked"></Button>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>