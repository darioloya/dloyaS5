<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dloyaS5.ActualizarEliminar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Actualizar Eliminar!" />

            <Entry x:Name="txtCodigo"/>
            <Entry x:Name="txtNombre"/>
            <Entry x:Name="txtApellido"/>
            <Entry x:Name="txtEdad"/>


            <Button Text="Actualizar" x:Name="btnActualizar" Clicked="btnActualizar_Clicked"></Button>
            <Button Text="Eliminar" x:Name="btnEliminar" Clicked="btnEliminar_Clicked"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>